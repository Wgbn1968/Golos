<?xml version="1.0" encoding="utf-8"?>
<!--AvailableCommands.xml-->
<!--This should be in the same folder as the plugin dll-->
<AvailableCommands>
    <command>
        <name>Sonos</name>
        <commandString />
        <info>
            <params />
            <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos</helplink>
            <description>
                <default>Control over Sonos wireless audio systems.</default>
            </description>
        </info>
        <command>
            <name>ClearQueue</name>
            <commandString>Sonos.ClearQueue</commandString>
            <info>
                <params>0</params>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#ClearQueue</helplink>
                <description>
                    <default>Remove all tracks from the current queue for the current player/coordinator.</default>
                </description>
            </info>
        </command>
        <command>
            <name>CreateGroup</name>
            <commandString>Sonos.CreateGroup</commandString>
            <info>
                <params>2-4</params>
                <parameter name="GroupName" type="string" optional="False">Existing Group or Single player</parameter>
                <parameter name="Player1" type="string" optional="False">First player to add</parameter>
                <parameter name="Player2" type="string" optional="True">Second player to add</parameter>
                <parameter name="Player3" type="string" optional="True">Third player to add (add more params if needed)</parameter>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#CreateGroup</helplink>
                <description>
                    <default>Creates a group or adds to an existing group.

Returns the name of the new group if successfully created.

You may include more than 3 players by adding new parameter boxes using the [[Parameter Helper]].</default>
                </description>
            </info>
        </command>
        <command>
            <name>DissolveAllGroups</name>
            <commandString>Sonos.DissolveAllGroups</commandString>
            <info>
                <params>0</params>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#DissolveAllGroups</helplink>
                <description>
                    <default>Dissolves (removes) all known groups freeing the individual players from the groups they were in, if any.</default>
                </description>
            </info>
        </command>
        <command>
            <name>DissolveGroup</name>
            <commandString>Sonos.DissolveGroup</commandString>
            <info>
                <params>1</params>
                <parameter name="Group Name" type="string" optional="False">Name of Group to dissolve</parameter>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#DissolveGroup</helplink>
                <description>
                    <default>Dissolves (removes) the group named &lt;Group Name&gt; freeing the individual players from the group.</default>
                </description>
            </info>
        </command>
        <command>
            <name>GenXML</name>
            <commandString>Sonos.GenXML</commandString>
            <info>
                <params>0</params>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GenXML</helplink>
                <description>
                    <default>Generates payloadXML files based on the Sonos library of items.
XML files will be created in the folder VoxCommando\SonosPayloads
Individual song titles are not generated by default because this can be a very slow process.  You can enable the generation of song title payloads in the Sonos plugin settings window.</default>
                </description>
            </info>
        </command>
        <command>
            <name>GetCurrentURI</name>
            <commandString>Sonos.GetCurrentURI</commandString>
            <info>
                <params>0</params>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GetCurrentURI</helplink>
                <description>
                    <default>Returns the current URI and Meta values.  This can be a useful way to figure out how to queue content using the Sonos.SetURI action.</default>
                </description>
            </info>
        </command>
        <command>
            <name>GetGroupInfo</name>
            <commandString>Sonos.GetGroupInfo</commandString>
            <info>
                <params>0-1</params>
                <parameter name="Player" type="string" optional="True" />
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GetGroupInfo</helplink>
                <description>
                    <default>Returns the name of the group for the Sonos &lt;Player&gt; if specified.

Otherwise it returns the name of the group for the current player.</default>
                </description>
            </info>
        </command>
        <command>
            <name>GetFavourites</name>
            <commandString>Sonos.GetFavourites</commandString>
            <info>
                <params>0</params>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GetFavourites</helplink>
                <description>
                    <default>Returns information about all favourites, including the URI and Meta for each one.  This can be a usefulway to figure out how to queue music using the Sonos.SetURI action.</default>
                </description>
            </info>
        </command>
        <command>
            <name>PartyMode</name>
            <commandString>Sonos.PartyMode</commandString>
            <info>
                <params>0-1</params>
                <parameter name="Coordinator" type="string" optional="True" />
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#PartyMode</helplink>
                <description>
                    <default>Create a group containing all available players.  If &lt;Coordinator&gt; is not specified then the current player will be used as the &lt;Coordinator&gt;.</default>
                </description>
            </info>
        </command>
        <command>
            <name>Play</name>
            <commandString>Sonos.Play</commandString>
            <info>
                <params />
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Play</helplink>
                <description>
                    <default>Actions to play various types of content on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.

See also Sonos.SetURI for additional ways to play content.</default>
                </description>
            </info>
            <command>
                <name>Album</name>
                <commandString>Sonos.Play.Album</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Album" type="string" optional="False">Name of album to queue and play</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Album</helplink>
                    <description>
                        <default>Play Sonos library songs from a given &lt;Album&gt; on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>AlbumArtist</name>
                <commandString>Sonos.Play.AlbumArtist</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="AlbumArtist" type="string" optional="False">Name of album artist to queue and play</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#AlbumArtist</helplink>
                    <description>
                        <default>Play Sonos library songs by &lt;AlbumArtist&gt; on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Artist</name>
                <commandString>Sonos.Play.Artist</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Artist" type="string" optional="False">Name of artist to queue and play</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Artist</helplink>
                    <description>
                        <default>Play Sonos library songs by &lt;Artist&gt; on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Composer</name>
                <commandString>Sonos.Play.Composer</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Composer" type="string" optional="False">Name of composer to queue and play</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Composer</helplink>
                    <description>
                        <default>Play Sonos library songs by &lt;Composer&gt; on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Favourite</name>
                <commandString>Sonos.Play.Favourite</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Favourite" type="string" optional="False">Name of the Sonos Favourite</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Favourite</helplink>
                    <description>
                        <default>Play Sonos library songs that were previously saved as a Sonos Favourite.

The tracks will be played on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Genre</name>
                <commandString>Sonos.Play.Genre</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Genre" type="string" optional="False">Specify content to queue and play</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Genre</helplink>
                    <description>
                        <default>Play Sonos library songs matching the &lt;Genre&gt; on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SavedQ</name>
                <commandString>Sonos.Play.SavedQ</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="SavedQ" type="string" optional="False">Specify content to queue and play</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SavedQ</helplink>
                    <description>
                        <default>Play Sonos library songs that were previously saved to a Sonos Queue.

The tracks will be played on the current player.  If the player is the coordinator of a group, then all players in that group will also play the content.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Share</name>
                <commandString>Sonos.Play.Share</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Share" type="string" optional="False">Path for a top level folder that was shared with the Sonos</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Share</helplink>
                    <description>
                        <default>Example of a share:

\\NAS\music

will play all the music stored in \\NAS\music and its subfolders.

If you only have created one share then this equates to "play everything".</default>
                    </description>
                </info>
            </command>
            <command>
                <name>TuneIn</name>
                <commandString>Sonos.Play.TuneIn</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="StationID" type="string" optional="False">Sonos radio station ID #</parameter>
                    <parameter name="Clear Queue" type="boolean" optional="True">Clear existing queue before adding new items</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#TuneIn</helplink>
                    <description>
                        <default>Radio station IDs for your favourite radio stations can be found in the payloadXML file after generating XML for your Sonos library: .\SonosPayloads\MyRadio.xml</default>
                    </description>
                </info>
            </command>
        </command>
        <command>
            <name>Player</name>
            <commandString />
            <info>
                <params />
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Player</helplink>
                <description>
                    <default>Basic control of the currently selected player (device) or group.</default>
                </description>
            </info>
            <command>
                <name>AdjustGrpVol</name>
                <commandString>Sonos.Player.AdjustGrpVol</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Adjustment" type="integer" optional="False">+/- %</parameter>
                    <parameter name="ReturnVol" type="boolean" optional="True">Return current volume. Default: False</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#AdjustGrpVol</helplink>
                    <description>
                        <default>Adjusts the current group's play volume by a positive or negative value in percent.

*** Caution is advised if setting volume above 50!  Avoid using loops or payloads that could increase the volume to a high level accidentally.

If &lt;ReturnVol&gt; is set to true the resulting volume will be returned as {LastResult}</default>
                    </description>
                </info>
            </command>
            <command>
                <name>AdjustVol</name>
                <commandString>Sonos.Player.AdjustVol</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Adjustment" type="integer" optional="False">+/- %</parameter>
                    <parameter name="ReturnVol" type="boolean" optional="True">Return current volume. Default: False</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#AdjustVol</helplink>
                    <description>
                        <default>Adjusts the current player's play volume by a positive or negative value in percent.

*** Caution is advised if setting volume above 50!  Avoid using loops or payloads that could increase the volume to a high level accidentally.

If &lt;ReturnVol&gt; is set to true the resulting volume will be returned as {LastResult}</default>
                    </description>
                </info>
            </command>
            <command>
                <name>GetStatus</name>
                <commandString>Sonos.Player.GetStatus</commandString>
                <info>
                    <params>0</params>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GetStatus</helplink>
                    <description>
                        <default>Get the current player's status (Playing or Stopped).</default>
                    </description>
                </info>
            </command>
            <command>
                <name>GetVolume</name>
                <commandString>Sonos.Player.GetVolume</commandString>
                <info>
                    <params>0</params>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GetVolume</helplink>
                    <description>
                        <default>Get the current player's volume (0-100%)</default>
                    </description>
                </info>
            </command>
            <command>
                <name>GetGrpVol</name>
                <commandString>Sonos.Player.GetGrpVol</commandString>
                <info>
                    <params>0-1</params>
                    <parameter name="GetList" type="boolean" optional="True">default: False</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#GetGrpVol</helplink>
                    <description>
                        <default>Returns the group volume (average volume of all players in group).

If &lt;GetList&gt; is True then matches will be returned with:
   {Match.1.1} = Player1 name , {Match.1.2} = Player1 volume
   {Match.2.1} = Player2 name , {Match.2.2} = Player2 volume
   {Match.3.1} etc. for any other players in group</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Next</name>
                <commandString>Sonos.Player.Next</commandString>
                <info>
                    <params>0</params>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Next</helplink>
                    <description>
                        <default>Advance the player to the next track in the queue (if any).</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Pause</name>
                <commandString>Sonos.Player.Pause</commandString>
                <info>
                    <params>0</params>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Pause</helplink>
                    <description>
                        <default>Pause or stop playback of the current player.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Play</name>
                <commandString>Sonos.Player.Play</commandString>
                <info>
                    <params>0</params>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Play</helplink>
                    <description>
                        <default>Start playback of the current queue (if any).</default>
                    </description>
                </info>
            </command>
            <command>
                <name>Previous</name>
                <commandString>Sonos.Player.Previous</commandString>
                <info>
                    <params>0</params>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Previous</helplink>
                    <description>
                        <default>Return the player to the previously played track in the queue (if any).

Does not rewind to the beginning of the current track.  
This can be accomplished using Sonos.Player.SeekTime  00:00:00</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SeekTime</name>
                <commandString>Sonos.Player.SeekTime</commandString>
                <info>
                    <params>1</params>
                    <parameter name="Time" type="string" optional="False">format  HH:MM:SS</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SeekTime</helplink>
                    <description>
                        <default>Seeks in the currently playing track to the &lt;Time&gt; specified.

Time must be specified in the format:
 HH:MM:SS</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SeekTrack</name>
                <commandString>Sonos.Player.SeekTrack</commandString>
                <info>
                    <params>1</params>
                    <parameter name="TrackIndex" type="integer" optional="False">Track number to play</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SeekTrack</helplink>
                    <description>
                        <default>Seeks in the current play queue to the track specified by &lt;TrackIndex&gt;

Note: the first &lt;TrackIndex&gt; # is 1, not 0.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SetGrpVol</name>
                <commandString>Sonos.Player.SetGrpVol</commandString>
                <info>
                    <params>1</params>
                    <parameter name="Volume" type="integer" optional="False">0 - 100%</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetGrpVol</helplink>
                    <description>
                        <default>Set the current group's play volume to a value from 0 to 100 percent.

*** Caution is advised if setting volume above 50!</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SetLoudness</name>
                <commandString>Sonos.Player.SetLoudness</commandString>
                <info>
                    <params>1</params>
                    <parameter name="Enabled" type="boolean" optional="False">True or False</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetLoudness</helplink>
                    <description>
                        <default>Set the current player's loudness to true or false (enabled disabled)</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SetPlayMode</name>
                <commandString>Sonos.Player.SetPlayMode</commandString>
                <info>
                    <params>1</params>
                    <parameter name="PlayMode" type="string" optional="False">eg: SHUFFLE_NOREPEAT</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetPlayMode</helplink>
                    <description>
                        <default>Sets the playback mode to one of:
SHUFFLE, SHUFFLE_NOREPEAT, NORMAL, REPEAT_ALL</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SetVol</name>
                <commandString>Sonos.Player.SetVol</commandString>
                <info>
                    <params>1</params>
                    <parameter name="Volume" type="integer" optional="False">0 - 100%</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetVol</helplink>
                    <description>
                        <default>Set the current play volume to a value from 0 to 100 percent.

***Caution is advised if setting volume above 50!</default>
                    </description>
                </info>
            </command>
        </command>
        <command>
            <name>SetLastPlayer</name>
            <commandString>Sonos.SetLastPlayer</commandString>
            <info>
                <params>0</params>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetLastPlayer</helplink>
                <description>
                    <default>Select the last current player which will be controlled by subsequent actions.

This allows you to temporarily target a player as follows.  Assume you are using player 'A' and want to temporarily control player 'C':

Sonos.SetPlayer C
... do various actions here ...
... do various actions here ...
Sonos.SetLastPlayer</default>
                </description>
            </info>
        </command>
        <command>
            <name>SetPlayer</name>
            <commandString>Sonos.SetPlayer</commandString>
            <info>
                <params>1</params>
                <parameter name="Player Name" type="string" optional="False">Player to control</parameter>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetPlayer</helplink>
                <description>
                    <default>Select the current player which will be controlled by subsequent actions.</default>
                </description>
            </info>
        </command>
        <command>
            <name>SetURI</name>
            <commandString>Sonos.SetURI</commandString>
            <info>
                <params>1-2</params>
                <parameter name="URI" type="string" optional="False">URI to play: see action description.</parameter>
                <parameter name="Meta" type="string" optional="True">Only required for some URIs</parameter>
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SetURI</helplink>
                <description>
                    <default>Low-level action for playing or queuing items manually.  

You can discover new working URI and Meta parameters using Sonos.GetCurrentURI and Sonos.GetFavourites

Example:
Track from library share:
URI: x-file-cifs://NAS/music/mp3/Air/Moon%20Safari/01%20La%20Femme%20d'argent.mp3
Meta: &lt;no parameter required&gt;
</default>
                </description>
            </info>
        </command>
        <command>
            <name>TTS</name>
            <commandString />
            <info>
                <params />
                <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#TTS</helplink>
                <description>
                    <default>Text To Speech actions</default>
                </description>
            </info>
            <command>
                <name>Speak</name>
                <commandString>Sonos.TTS.Speak</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Text" type="string" optional="False">TTS text to say aloud</parameter>
                    <parameter name="Volume" type="integer" optional="True">Temporary volume for current player only</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#Speak</helplink>
                    <description>
                        <default>Creates a wav file with &lt;Text&gt; spoken by the current Sonos TTS voice, and plays it on the current Sonos Player Device.

***WARNING: The voice might need to be different from the one being used by the normal VoxCommando TTS plugin.  We hope to fix this.  It depends on the TTS voice.</default>
                    </description>
                </info>
            </command>
            <command>
                <name>SpeakSync</name>
                <commandString>Sonos.TTS.SpeakSync</commandString>
                <info>
                    <params>1-2</params>
                    <parameter name="Text" type="string" optional="False">TTS text to say aloud</parameter>
                    <parameter name="Volume" type="integer" optional="True">Temporary volume for current player only</parameter>
                    <helplink>http://voxcommando.com/mediawiki/index.php?title=Plugin_Sonos#SpeakSync</helplink>
                    <description>
                        <default>Creates a wav file with &lt;Text&gt; spoken by the current Sonos TTS voice, and plays it on the current Sonos Player Device.

***WARNING: The voice might need to be different from the one being used by the normal VoxCommando TTS plugin.  We hope to fix this.  It depends on the TTS voice.

Waits for Sonos to finish speaking and resuming music before continuing to the next action.</default>
                    </description>
                </info>
            </command>
        </command>
    </command>
</AvailableCommands>